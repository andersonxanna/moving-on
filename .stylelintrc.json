{
  "extends": [
    "stylelint-config-standard"
  ],
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "selector-type-no-unknown": [
      true,
      {
        "ignore": [
          "custom-elements",
          "default-namespace"
        ]
      }
    ]
  }
}